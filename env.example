# MediFlow Environment Configuration
# Copy this file to .env and update the values

# Development Settings
VITE_USE_MOCK_DATA=true

# Monitoring Configuration
# VITE_SENTRY_DSN=your_sentry_dsn_here
# VITE_APP_VERSION=1.0.0

# Optional: Custom API endpoints (if different from default)
# VITE_API_BASE_URL=https://api.mediflow.com

# Optional: Feature flags
# VITE_ENABLE_ANALYTICS=false
# VITE_ENABLE_DEBUG_MODE=true

# Optional: External service URLs
# VITE_FILE_UPLOAD_URL=https://files.mediflow.com
# VITE_EMAIL_SERVICE_URL=https://email.mediflow.com
