# MediFlow Production Environment Configuration
# This file contains production environment variables

# Base44 Configuration (REQUIRED)
VITE_BASE44_SERVER_URL=https://base44.app
VITE_BASE44_APP_ID=your_production_app_id_here
VITE_USE_MOCK_DATA=false

# API Configuration
VITE_API_BASE_URL=https://api.yourdomain.com
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_DEBUG_MODE=false

# Security Configuration
VITE_ENABLE_SECURITY_HEADERS=true
VITE_FORCE_HTTPS=true

# Monitoring Configuration
VITE_SENTRY_DSN=your_sentry_dsn_here
VITE_APP_VERSION=1.0.0

# External Service URLs
VITE_FILE_UPLOAD_URL=https://files.yourdomain.com
VITE_EMAIL_SERVICE_URL=https://email.yourdomain.com

# Performance Configuration
VITE_ENABLE_COMPRESSION=true
VITE_ENABLE_CACHING=true
